package userDao;
import model.*;

import java.sql.Connection;
import java.sql.SQLException;
import java.sql.Statement;
public class UserService {
	static java.sql.Connection con = (Connection) ConnectionString.getCon();
	public static void insertUser(User user)
	{
		try {
			Statement stmt = con.createStatement();
			stmt.executeUpdate("insert into user(name,gender,username,password) values'"+user.getName()+"','"+user.getGender()+"','"+user.getUsername()+\"','\"+p.getPassword()+\"') ");
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
